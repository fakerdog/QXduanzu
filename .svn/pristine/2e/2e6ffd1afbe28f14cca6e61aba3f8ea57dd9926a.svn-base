/* pages/SharePhoto/SharePhoto.wxss */
.drawer_content_canvas{
  width: 1050px;
  height: 1770px;
}
.hidden-view{
  display: none;
}
Page{
  height: 100%;
  width: 100%;
  background-color: #fff;
  margin: 0;
  padding: 0;
}
.tempview{
  width: 100%;
  height: 100%;
  margin: 0;
  padding:0;
  overflow: hidden;
}
.drawer_btn_block{
  left: 0px;
  bottom: 0px;
  position: fixed;
  width: 100%;
  height: 85px;
  align-items: center;
  justify-content: center;
  display:flex;
	flex-direction:row;
  background-color: rgba(255, 255, 255, 0.7);
  padding-top:5px;
}
.drawer_btn_block button{
  height: 80rpx;
  display: flex;
  line-height: 80rpx;
  flex-direction: column;
  align-items: center;
  font-size: 14px;
  color: #000;
  border-radius: 10px;
  background-color: rgba(255, 255, 255, 0);
}
.drawer_btn_block button:nth-child(1){
  background-color: #CED2E2;
}
.drawer_btn_block button:nth-child(2){
  background-color: #F2ED84;
}
